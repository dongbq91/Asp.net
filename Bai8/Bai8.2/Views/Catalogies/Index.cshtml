@model IEnumerable<Bai8._2.Models.Catalogy>

@{
    ViewBag.Title = "Index";
}

<h2>Danh Mục</h2>

<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CatalogyID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CatalogyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.CatalogyID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.CatalogyName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Description)
    </td>
    <td>
        @Html.ActionLink("Sửa", "Edit", new { id = item.CatalogyID.Trim() }) |
        @Html.ActionLink("Xem", "Details", new { id = item.CatalogyID.Trim() }) |
        @Html.ActionLink("Xóa", "Delete", new { id = item.CatalogyID.Trim() })
    </td>
</tr>
}

</table>
