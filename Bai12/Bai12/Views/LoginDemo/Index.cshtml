
@{
    ViewBag.Title = "Index";
}

<h2>Login</h2>
@using (Html.BeginForm("CheckLogin", "LoginDemo"))
{
    <div>
        @Html.Label("Username")
    </div>
    <div>
        @Html.TextBox("username")
    </div>
    <div >
        @Html.Label("Password")
    </div>
    <div>
    @Html.TextBox("password")
    </div>
    <br />
    <input type="submit" value="Login" />
    <br />
    @ViewBag.msg

}
